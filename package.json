  "scripts": {
    "_start": "DEBUG=app,sql,appInit,resourceapi,links,taxonomies,terms,jwt,datautil,comments nodemon -L ./bin/www",
    "start": "DEBUG=*,-axm:*,-express:*,-babel,-babel:* NODE_ENV=production supervisor -w ./server,./bin ./bin/www",
    "serve-dev": "set NODE_ENV=development&& gulp dev-env",
    "development": "set NODE_ENV=development&& node ./bin/www",
    "serve-sta": "set NODE_ENV=staging&& gulp dev-env",
    "serve-prod": "set NODE_ENV=production&& gulp dev-env",
    "dev-build": "gulp dev-build",
    "sta-build": "gulp sta-env",
    "staging": "NODE_ENV=staging node ./bin/www",
    "prod": "NODE_ENV=production node ./bin/www",
    "build": "gulp prod-env",
    "import-dev": "node ./bin/scripts",
    "import-sta": "NODE_ENV=staging node ./bin/scripts",
    "import-prod": "NODE_ENV=production node ./bin/scripts",
    "migrate-dev": "MUST_DEBUG=true IS_MIGRATING=true DEBUG=*,-axm:*,-express:*,-babel node ./bin/migrations",
    "migrate-sta": "NODE_ENV=staging MUST_DEBUG=true IS_MIGRATING=true DEBUG=*,-axm:*,-express:*,-babel node ./bin/migrations",
    "migrate-prod": "NODE_ENV=production MUST_DEBUG=true IS_MIGRATING=true DEBUG=*,-axm:*,-express:*,-babel,-sequelize:*,-Sequelize:*,,-Sequelize node ./bin/migrations",
    "seed-all-dev": "NODE_ENV=development node_modules/.bin/sequelize db:seed:all",
    "seed-all-sta": "NODE_ENV=staging node_modules/.bin/sequelize db:seed:all",
    "seed-all-prod": "NODE_ENV=production node_modules/.bin/sequelize db:seed:all",
    "test-react": "jest --no-cache --config jest-config.js ./public/assets/scripts",
    "test-react-watch": "jest --watch --no-cache --config jest-config.js ./public/assets/scripts"
  },
  "browserify": {
    "transform": [
      [
        "babelify",
        {
          "presets": [
            "@babel/preset-env",
            "@babel/preset-react"
          ]
        }
      ],
      "envify"
    ]
  },
